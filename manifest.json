{
    "manifest_version": 3,
    "name": "Popup chrome extension boilerplate",
    "description": "Boilerplate to start building chrome extension",
    "version": "1.0.0",
    "action": {
        "default_icon": {
            "16": "images/icon16.png",
            "24": "images/icon24.png",
            "32": "images/icon32.png"
        },
        "default_title": "Shown in tooltip",
        "default_popup": "popup.html"
    }
}